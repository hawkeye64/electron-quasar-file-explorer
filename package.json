  "description": "An Electron File Explorer created with Quasar",
  "productName": "File Explorer",
  "author": "Jeff Galbraith <jeff@quasar.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue src",
    "lint-fix": "eslint --ext .js,.vue src --fix",
    "dev": "quasar dev -m electron",
